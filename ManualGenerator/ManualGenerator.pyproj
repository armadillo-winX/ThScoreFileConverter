<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>3fde820f-b99d-4936-ac8e-11a36697f9d1</ProjectGuid>
    <ProjectHome>.</ProjectHome>
    <StartupFile>ManualGenerator.py</StartupFile>
    <SearchPath>
    </SearchPath>
    <WorkingDirectory>.</WorkingDirectory>
    <Name>ManualGenerator</Name>
    <RootNamespace>ManualGenerator</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
    <OutputPath>Debug</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugSymbols>false</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
    <OutputPath>Release</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ManualGenerator.py" />
  </ItemGroup>
  <ItemGroup>
    <None Include="format.md" />
    <None Include="manual.md" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="requirements.txt" />
  </ItemGroup>
  <PropertyGroup>
    <VisualStudioVersion Condition="'$(VisualStudioVersion)' == ''">10.0</VisualStudioVersion>
    <PtvsTargetsFile>$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)\Python Tools\Microsoft.PythonTools.targets</PtvsTargetsFile>
  </PropertyGroup>
  <Import Condition="Exists($(PtvsTargetsFile))" Project="$(PtvsTargetsFile)" />
  <Import Condition="!Exists($(PtvsTargetsFile))" Project="$(MSBuildToolsPath)\Microsoft.Common.targets" />
  <!-- Uncomment the CoreCompile target to enable the Build command in
       Visual Studio and specify your pre- and post-build commands in
       the BeforeBuild and AfterBuild targets below. -->
  <Target Name="CoreCompile">
    <RunPythonCommand Target="$(StartupFile)"
                      TargetType="script"
                      ExecuteIn="none"
                      Arguments="--input-dir=$(WorkingDirectory) --output-dir=$(OutputPath)"
                      ConsoleToMSBuild="false">
      <Output TaskParameter="ConsoleOutput" PropertyName="OutputText" />
      <Output TaskParameter="ConsoleError" PropertyName="ErrorText" />
    </RunPythonCommand>
  </Target>
  <Target Name="Clean">
    <RemoveDir Directories="$(OutputPath)" />
  </Target>
  <Target Name="BeforeBuild">
  </Target>
  <Target Name="AfterBuild">
  </Target>
  <PropertyGroup>
    <ResolveReferencesDependsOn />
    <PrepareResourcesDependsOn />
  </PropertyGroup>
</Project>