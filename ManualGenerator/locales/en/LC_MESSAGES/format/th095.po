msgstr ""

msgid "ハイスコア"
msgstr "High score"

msgid "``[2]``"
msgstr ""

msgid "登録してあるベストショットのスコア"
msgstr "Registered best shot score"

msgid "``[3]``"
msgstr ""

msgid "撮影枚数"
