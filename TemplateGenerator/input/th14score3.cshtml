<#@ template debug="false" hostspecific="false" language="C#" #>
<#@ include file="imports.ttinclude" once="true" #>
<#@ include file="footer.ttinclude" once="true" #>
<#@ import namespace="TemplateGenerator.Models.Th14" #>
<#@ output extension=".html" encoding="utf-8" #>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ja" lang="ja">

 <head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><#= Definitions.Title #> スコアデータ</title>
  <link rel="stylesheet" href="style.css" />
<style>
/* <![CDATA[ */
    td.score { min-width: 70px; }
    td.stage { min-width: 70px; }
    table.practice { float: left; }
/* ]]> */
</style>
 </head>

 <body>

  <header>
   <h1><#= Definitions.Title #> スコアデータ</h1>
   <hr />
   <nav>
    <ul>
     <li><a href="th14score1.html">スコアデータ全般</a></li>
     <li><a href="th14score2.html">個別ランキング</a></li>
     <li>プラクティス</li>
     <li><a href="th14card1.html">御札戦歴（<#= Definitions.CareerKinds["S"] #>）</a></li>
     <li><a href="th14card2.html">御札戦歴（<#= Definitions.CareerKinds["P"] #>）</a></li>
    </ul>
    <ul>
<#
    foreach (var level in Definitions.LevelPracticeNames)
    {
#>
     <li><a href="#<#= level.Value #>"><#= level.Value #></a></li>
<#
    }
#>
    </ul>
   </nav>
  </header>

  <main>

<#
    foreach (var level in Definitions.LevelPracticeNames)
    {
#>
   <section>
    <h2 id="<#= level.Value #>"><#= level.Value #></h2>

<#
        foreach (var chara in Definitions.CharacterNames)
        {
#>
    <table class="practice">
     <caption><#= chara.Value #></caption>
     <thead>
      <tr>
       <th class="stage">Stage</th>
       <th class="score">Score</th>
      </tr>
     </thead>
     <tbody>
<#
            foreach (var stage in Definitions.StagePracticeNames)
            {
#>
      <tr>
       <td class="stage"><#= stage.Value #></td>
       <td class="score">%T14PRAC<#= level.Key #><#= chara.Key #><#= stage.Key #></td>
      </tr>
<#
            }
#>
     </tbody>
    </table>

<#
        }
#>
   </section>

<#
        if (level.Key != Definitions.LevelPracticeNames.Last().Key)
        {
#>
   <hr />

<#
        }
    }
#>
  </main>

  <footer>
   <p>
    Data output by <a href="<#= projectUrl #>"><#= projectName #></a>.
<script>
// <![CDATA[
    document.write("Last updated: ", document.lastModified, ".");
// ]]>
</script>
   </p>
  </footer>

 </body>
</html>
